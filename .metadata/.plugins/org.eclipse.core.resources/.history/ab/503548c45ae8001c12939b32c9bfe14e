package lokendrap;

public class Interview 
{
	public static void main(String[] args) 
	{
		String st="java, selenium, maven, git";
		
		String str[]=st.split(",");
		
		for(String s:str)
		{
			System.out.println(s);
		}
		
		System.out.println('\n');
		
		
		String s1="java";
		String s2="java script";
		String s3="java methods";
		
		String s4=s1+s2+s3;
		System.out.println(s4);
		
	}
	
	
	

}
